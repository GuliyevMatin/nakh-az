

<header>

    <div class="header">
        <div class="header-title">
            <h3>Naxçıvan Muxtar Respublikası</h3>
            <div class="header-language">
                <a href="">
                    <img src="/static/images/AZ.png" alt="">
                </a>
                <a href="">
                    <img src="/static/images/EN.png" alt="">
                </a>
                <a href="">
                    <img src="/static/images/RUS.png" alt="">
                </a>
            </div>
        </div>
        <div class="progress">
            <% menus.forEach((element,index) => { %>
            <div
              class="progress-bar"
              role="progressbar"
              aria-label="<%= element.slug %>"
           
              aria-valuenow="15"
              aria-valuemin="0"
              aria-valuemax="100"
            ></div>
            <% }) %>
          </div>
          
          <%- include('./navbar',menus)  %> 

    </div>

    <p class="header-date">Saat</p>
   
</header>